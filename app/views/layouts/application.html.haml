    -# %title Yummy - Count your diet calories
    -#
    -# / Bootstrap CSS
    -# %link{:href => "https://cdn.rawgit.com/twbs/bootstrap/v4-dev/dist/css/bootstrap.css", :rel => "stylesheet"}/
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = csrf_meta_tags
  -# %body
    / jQuery first, then Bootstrap JS.
    %script{:src => "https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"}
    %script{:src => "https://cdn.rawgit.com/twbs/bootstrap/v4-dev/dist/js/bootstrap.js"}

    / BEGIN THE YUMMY STRUCTURE
    %header
      %div.container
        %div.row
          %ul.nav.nav-tabs{ role: "tablist" }
            -# %li.nav-item
            -#   %img{ alt: "", src: "/images/yummy_apple.png", style: "width: 40px; height: 40px;" }/
            %li.nav-item
              %a.nav-link{ href: '#' } home
            %li.nav-item.dropdown
              %a.nav-link.dropdown-toggle{ "data-toggle" => "dropdown", :href => "#", :role => "button", "aria-haspopup" => true, "aria-expanded" => false } search

              %div.dropdown-menu
                %a.dropdown-item{ href: '#' } Action
                %a.dropdown-item{ href: '#' } Another Action
            %li.nav-item
              %a.nav-link{ href: '#' } about
            %li.nav-item
              %a.nav-link.disabled.text-muted{ href: '#' } contact

    %div#food_search.container
      .form-inline.navbar-form
        = form_for(:aliment, url: :search_aliments, html: { id: 'search_bar_form' }, method: :get) do |f|
          .row
            .col-lg-6
              .input-group{ style: "width: 800px" }
                = f.autocomplete_field :name, autocomplete_aliment_name_aliments_path, placeholder: 'search', class: "form-control m-l-md", id: 'search_bar_input'

                %span.input-group-btn
                  = f.submit 'search', class: "button btn btn-success", id: 'search_bar_button'

        = yield
