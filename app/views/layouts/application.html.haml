            :javascript
              (function() {
                var
                  _activeNavItem = null,
                  _activeSubMenu = null;

                function _openMenu(navItem, subMenu) {
                  navItem.className = navItem.className.concat(' open');
                  subMenu.className = subMenu.className.concat(' active');
                }

                function _closeMenu(navItem, subMenu) {
                  navItem.className = navItem
                    .className
                    .replace(/\bopen\b/, '')
                    .replace(/^\s+|\s+$/g, '');

                  subMenu.className = subMenu
                    .className
                    .replace(/\bactive\b/, '')
                    .replace(/^\s+|\s+$/g, '');
                }

                function _bindToggleSubMenu(navItem, subMenu) {
                  var
                    _toggleSubMenu = function(navItem, subMenu) {
                      if ( navItem.className.indexOf('open') >= 0 ) {
                        _closeMenu(_activeNavItem, _activeSubMenu);
                      } else {
                        if ( ((null !== _activeNavItem) && (null !== _activeSubMenu)) && (_activeNavItem !== navItem) ) {
                          _closeMenu(_activeNavItem, _activeSubMenu);
                        }
                        _openMenu(navItem, subMenu);
                        _activeNavItem = navItem;
                        _activeSubMenu = subMenu;
                      }
                    };

                  if ( window.addEventListener ) {
                    navItem.addEventListener(
                      'click',
                      function() {
                        _toggleSubMenu(navItem, subMenu)
                      },
                      true
                    );
                  } else if ( window.attachEvent ) {
                    navItem.attachEvent(
                      'onclick',
                      function() {
                        _toggleSubMenu(navItem, subMenu)
                      }
                    );
                  }
                }

                (function() {
                  var
                    siteNav = document.getElementById('site-nav'),
                    navItem = null,
                    subMenu = null,
                    i,
                    k,
                    onOutsideClickCloseMenu = function(event) {
                      var
                        e = event || window.event;

                      if ( (_activeSubMenu !== null)
                        && ( (e.clientY < siteNav.offsetTop)
                          || (e.clientY > (_activeSubMenu.offsetTop + _activeSubMenu.offsetHeight))
                        ) )
                      {
                        _closeMenu(_activeNavItem, _activeSubMenu);
                      }
                    };

                  for ( i = 0; i < siteNav.childNodes.length; i += 1 ) {
                    if ( (1 === siteNav.childNodes[i].nodeType)
                      && ('LI' === siteNav.childNodes[i].tagName)
                      && (siteNav.childNodes[i].className.indexOf('nav-item') >= 0) )
                    {
                      for ( k = 0; k < siteNav.childNodes[i].childNodes.length; k += 1 ) {
                        if ( (1 === siteNav.childNodes[i].childNodes[k].nodeType)
                          && (siteNav.childNodes[i].childNodes[k].tagName === 'A')
                          && (siteNav.childNodes[i].childNodes[k].className.indexOf('parent-link') >= 0) )
                        {
                          navItem = siteNav.childNodes[i].childNodes[k];
                          subMenu = navItem;
                          do {
                            subMenu = subMenu.nextSibling;
                          } while (
                            ( null !== subMenu )
                            && !(
                              (1 === subMenu.nodeType)
                              && ('UL' === subMenu.tagName)
                              && (subMenu.className.indexOf('sub-nav') >= 0)
                            )
                          );
                          _bindToggleSubMenu(navItem, subMenu);
                        }
                      }
                    }
                  }

                  if ( window.addEventListener ) {
                    document.getElementsByTagName('html')[0].addEventListener(
                      'click',
                      onOutsideClickCloseMenu,
                      true
                    );
                  } else if ( window.attachEvent ) {
                    document.getElementsByTagName('html')[0].attachEvent(
                      'onclick',
                      onOutsideClickCloseMenu
                    );
                  }
                }());
              }());

