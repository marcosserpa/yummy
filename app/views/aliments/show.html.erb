    <div class="l-colspan--l colspan--l--has-border">
      <div class="gem__intro">
        <div id="markup" class="gem__desc">
          <% if @aliment.description %>
            <p><%= @aliment.description %></p>
          <% else %>
            <p>This aliment has no description.</p>
          <% end %>
        </div>
      </div>

