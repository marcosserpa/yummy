<% if @aliment.present? %>
  <% @title = @aliment.name %>
  <% @subtitle = @aliment.proximate.try(:energy) ? "#{ @aliment.proximate.try(:energy).round(2) } calories" : '--' %>

  <% content_for :head do %>
    <%= auto_discovery_link_tag(:atom, aliment_path(@aliment), { title: "#{@aliment.name} Calories Feed" }) %>
    <% if @aliment.proximate.try(:energy).present? %>
      <link rel="canonical" href="<%= aliment_path(@aliment) %>" />
    <% else %>
      <meta name="robots" content="noindex" />
    <% end %>
  <% end %>

