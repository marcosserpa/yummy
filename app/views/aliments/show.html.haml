                  %td{ colspan: "3" }
                    Calories from fat
                    %span{ class: "amount" } 23
                %tr{ class: "daily-value-header" }
                  %td{ colspan: "3" }
                    %span{ class: "label" } % Daily Value
                    %span{ class: "footnote" } 1
