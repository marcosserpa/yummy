    %div{ id: "mData" }
      %div{ style: "overflow: auto;" }
        %div{ id: "mNutrient" }
          %div{ class: "nutrient-facts" }
            %table
              %tbody
                %tr
                  %th{ colspan: "3" }
                    Nutrition Facts
                %tr{ class: "energy" }
                  %td{ class: "calories" }
                    Calories&nbsp;
                    %span{ class: "amount" } 60
                  %td{ class: "kilojoules", colspan: "2" }
                    (
                    %span{ class: "amount" } 251
